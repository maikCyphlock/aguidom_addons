<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_partner_form_podium" model="ir.ui.view">
        <field name="name">res.partner.form.podium</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="club_id" options="{'no_create': True}"/>
                <field name="is_podium_athlete"/>
                <field name="is_podium_representative"/>
                <field name="is_podium_coach"/>
                <field name="is_podium_manager"/>
            </xpath>
        </field>
    </record>
</odoo>
